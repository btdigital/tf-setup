{
    "project": "lambda-arch-project",
    "gcs_buckets": {
        "europe-west6": {
            "name": ["raw-datalake", "sensitive-datalake"],
            "storage_class": "",
            "versioning_enabled": true,
            "lifecycle_rule": {
                "ttl": {
                    "condition_age": 30,
                    "action_type": "Delete"
                }
            },
            "internal_tenant_roles_admin": {
                "objectAdmin": {
                    "service_accounts": ["platform-infra", "platform-ko"]
                }
            },
            "internal_tenant_roles_viewer": {
                "objectViewer": {
                    "service_accounts": ["viewer-infra", "viewer-ko"]
                }
            },
            "internal_tenant_roles_creator": {
                "objectCreator": {
                    "service_accounts": ["creator-infra", "creator-ko"]
                    }
            },
            "internal_tenant_roles_storage": {
                "storageAdmin":{
                        "service_accounts": ["bucket-infra", "bucket-ko"]
                    }
            }
        }
    }
}